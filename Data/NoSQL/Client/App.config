<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IRavenDBProvider" 
                         openTimeout="00:30:00"
                         closeTimeout="00:30:00"
                         sendTimeout="00:30:00"
                         receiveTimeout="00:30:00"/>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8733/DataService/RavenDBService/"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IRavenDBProvider"
                contract="RavenDBProvider.IRavenDBProvider" name="NetTcpBinding_IRavenDBProvider">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>